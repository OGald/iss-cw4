#!/bin/bash

./strip-image \
	-i iss2022/g42-db_i:latest \
	-t iss2022/g42-db_i_stripped_newdock:2  \
     	-p mysql-common \
	-p mariadb-common \
	-p mariadb-client-core-10.8 \
	-p mariadb-server-core-10.8 \
	-p mariadb-client-core-10.8 \
	-p mariadb-server-core-10.8 \
	-p mariadb-server \
	-p mariadb-client-10.8 \
	-p libc-bin \
	-p bash \
      	-d Dockerfile \
	-f /etc/passwd \
	-f /etc/group \
	-f '/usr/lib/*/libnss*' \
	-f /usr/lib64 \
	-f /usr/bin/groups \
	-f /usr/bin/ls \
	-f /usr/bin/du \
	-f /usr/bin/awk \
	-f /usr/bin/date \
	-f /usr/bin/mktemp \
	-f /usr/bin/id \
	-f /usr/bin/systemctl \
	-f /usr/bin/sudo \
	-f /usr/bin/* \
	-f /etc/hosts \
	-f '/usr/lib/*/libzip*' \
	-f /bin/sh \
	-f /run/mysqld \
	-f /run/mysqld/* \
	-f /bin/bash \
	-f /usr/bin/mariadb \
	-f /etc/mysql/mariadb.conf.d \
	-f /etc/mysql/conf.d \
	-f /etc/mysql/* \
	-f /sbin/* \
	-f /usr/bin/docker-entrypoint.sh \
	-f /docker-entrypoint-initdb.d \
	-f /var \
	-f /var/* \
	-f /lib/* \
	-f /usr/local \
	-f /usr/bin/docker-entrypoint.sh \
	-f /*
